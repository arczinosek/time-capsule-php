{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.344",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.4",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-bundle": "^3.4",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "ci:test": [
            "@phpcs",
            "@phpstan",
            "@test:unit",
            "@test:integration"
        ],
        "dev:fix": "@phpcbf",
        "dev:test": [
            "@phpcs",
            "@phpstan",
            "@test:unit:cov"
        ],
        "phpcs": "vendor/bin/phpcs -p --cache",
        "phpcbf": "vendor/bin/phpcbf",
        "phpstan": "vendor/bin/phpstan",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:cov": ["XDEBUG_MODE=coverage phpunit --coverage-html=var/test-coverage --coverage-text=/dev/stdout --only-summary-for-coverage-text"],
        "test:unit": ["phpunit --testsuit Unit"],
        "test:unit:cov": ["XDEBUG_MODE=coverage phpunit --testsuit Unit --coverage-html=var/test-coverage --coverage-text=/dev/stdout --only-summary-for-coverage-text"],
        "test:integration": ["phpunit --testsuit Integration"]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.2",
        "squizlabs/php_codesniffer": "^3.13",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.63"
    }
}
